	sub	A,	$00		; sub		$00	;    D6 00
	sub	A,	A		; sub.a			;    97
	sub	A',	A'		; sub.a'		;    97
	sub	A,	B		; sub.b			;    90
	sub	A,	B'		; sub.b'		;    90
	sub	A,	C		; sub.c			;    91
	sub	A,	C'		; sub.c'		;    91
	sub	A,	D		; sub.d			;    92
	sub	A,	D'		; sub.d'		;    92
	sub	A,	E		; sub.e			;    93
	sub	A,	E'		; sub.e'		;    93
	sub	A,	H		; sub.h			;    94
	sub	A,	H'		; sub.h'		;    94
	sub	A,	L		; sub.l			;    95
	sub	A,	L'		; sub.l'		;    95
	sub	A,	IXH		; sub.ixh		; DD 94
	sub	A,	IXL		; sub.ixl		; DD 95
	sub	A,	IYH		; sub.iyh		; FD 94
	sub	A,	IYL		; sub.iyl		; FD 95
	;-----------------------------------------------------------------------
xor:	xor	A, 	[HL]		; xor*hl		;    AE
	xor	A, 	[HL']		; xor*hl'		;    AE
	xor	A,	[IX+$00]	; xor*ix	$00	; DD AE 00
	xor	A,	[IY+$00]	; xor*iy	$00	; FD AE 00
	xor	A,	$00		; xor		$00	;    EE 00
	xor	A,	A		; xor.a			;    AF
	xor	A',	A'		; xor.a'		;    AF
	xor	A,	B		; xor.b			;    A8
	xor	A,	B'		; xor.b'		;    A8
	xor	A,	C		; xor.c			;    A9
	xor	A,	C'		; xor.c'		;    A9
	xor	A,	D		; xor.d			;    AA
	xor	A,	D'		; xor.d'		;    AA
	xor	A,	E		; xor.e			;    AB
	xor	A,	E'		; xor.e'		;    AB
	xor	A,	H		; xor.h			;    AC
	xor	A,	H'		; xor.h'		;    AC
	xor	A,	L		; xor.l			;    AD
	xor	A,	L'		; xor.l'		;    AD
	xor	A,	IXH		; xor.ixh		; DD AC
	xor	A,	IXL		; xor.ixl		; DD AD
	xor	A,	IYH		; xor.iyh		; FD AC
	xor	A,	IYL		; xor.iyl		; FD AD

; to match CP/M's behaviour for build tests,
; pad the binary to the next 128 byte boundary
;
end_of_code:
	.DSB	$80 - (end_of_code # $80), $00